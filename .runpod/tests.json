{
  "tests": [
    {
      "name": "test_base_model",
      "description": "Test base model with English audio (JFK speech)",
      "input": {
        "audio": "https://github.com/openai/whisper/raw/main/tests/jfk.flac",
        "model": "base",
        "language": "en",
        "temperature": 0.0,
        "beam_size": 5,
        "word_timestamps": true
      },
      "timeout": 120000,
      "validation": {
        "transcription": {
          "min_length": 10
        },
        "detected_language": "en",
        "device": "cuda"
      }
    }
  ],
  "config": {
    "gpuTypeIds": ["AMPERE_16", "NVIDIA RTX A4000", "NVIDIA RTX 4000 Ada Generation"],
    "gpuCount": 1,
    "env": [],
    "allowedCudaVersions": [
      "11.8",
      "12.0",
      "12.1",
      "12.2",
      "12.3",
      "12.4"
    ]
  }
}
